{
  "name": "timmywil.com",
  "private": true,
  "description": "Personal blog and website for Timmy Willison",
  "version": "1.4.3",
  "author": "Timmy Willison <4timmywil@gmail.com>",
  "scripts": {
    "build": "gatsby build",
    "deploy": "gatsby build && gh-pages -d public",
    "dev": "npm start",
    "format": "eslint . --fix && prettier --write \"**/*.tsx\" \"**/*.ts\" \"**/*.js\" \"**/*.json\" \"**/*.md\"",
    "lint": "eslint . && prettier --check \"**/*.md\" \"**/*.json\"",
    "post": "node tasks/post.js",
    "prepare": "husky install",
    "release": "release-it",
    "schema": "graphql-codegen --config codegen.yml",
    "serve": "gatsby serve",
    "start": "gatsby develop -H 0.0.0.0",
    "test": "npm run lint",
    "typescript": "tsc -p tsconfig.json --noEmit"
  },
  "dependencies": {
    "@emotion/babel-plugin": "^11.12.0",
    "@emotion/react": "^11.13.0",
    "@emotion/styled": "^11.13.0",
    "@gatsbyjs/reach-router": "^2.0.1",
    "caniuse-lite": "^1.0.30001643",
    "compass-vertical-rhythm": "^1.4.5",
    "gatsby": "^5.13.7",
    "gatsby-plugin-emotion": "^8.13.1",
    "gatsby-plugin-feed": "^5.13.1",
    "gatsby-plugin-google-analytics": "^5.13.1",
    "gatsby-plugin-image": "^3.13.1",
    "gatsby-plugin-manifest": "^5.13.1",
    "gatsby-plugin-offline": "^6.13.2",
    "gatsby-plugin-sharp": "^5.13.1",
    "gatsby-plugin-typescript": "^5.13.1",
    "gatsby-plugin-typography": "^5.13.1",
    "gatsby-remark-copy-linked-files": "^6.13.1",
    "gatsby-remark-images": "^7.13.1",
    "gatsby-remark-prismjs": "^7.13.1",
    "gatsby-remark-responsive-iframe": "^6.13.1",
    "gatsby-remark-smartypants": "^6.13.1",
    "gatsby-source-filesystem": "^5.13.1",
    "gatsby-transformer-remark": "^6.13.1",
    "gatsby-transformer-sharp": "^5.13.1",
    "prismjs": "^1.29.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-typography": "^0.16.23",
    "serialize-javascript": "^6.0.2",
    "typeface-rosario": "^1.1.13",
    "typescript": "^5.5.4",
    "typography": "^0.16.24",
    "typography-theme-twin-peaks": "^0.16.19"
  },
  "devDependencies": {
    "@babel/core": "^7.24.9",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/typescript": "^4.0.9",
    "@release-it/conventional-changelog": "^8.0.1",
    "@types/babel__core": "^7.20.5",
    "@types/compass-vertical-rhythm": "^1.4.5",
    "@types/node": "^20.14.12",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/typography": "^0.16.7",
    "@typescript-eslint/eslint-plugin": "^7.17.0",
    "@typescript-eslint/parser": "^7.17.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.35.0",
    "gatsby-cli": "^5.13.3",
    "gatsby-link": "^5.13.1",
    "gatsby-plugin-utils": "^4.13.1",
    "gatsby-script": "^2.13.0",
    "gh-pages": "^6.1.1",
    "graphql": "^16.9.0",
    "graphql-compose": "^9.0.11",
    "husky": "^9.1.1",
    "lodash.template": "^4.5.0",
    "mkdirp": "^3.0.1",
    "opentracing": "^0.14.7",
    "prettier": "^3.3.3",
    "release-it": "^17.6.0",
    "ts-node": "^10.9.2"
  },
  "resolutions": {
    "graphql-compose": "9.0.9"
  },
  "bugs": {
    "url": "https://github.com/timmywil/timmywil.com/issues"
  },
  "homepage": "https://github.com/timmywil/timmywil.com#readme",
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "main": "n/a",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/timmywil/timmywil.com.git"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "prettier": {
    "arrowParens": "always",
    "endOfLine": "auto",
    "printWidth": 100,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "none"
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint",
      "react",
      "prettier"
    ],
    "extends": [
      "plugin:@typescript-eslint/recommended",
      "plugin:react/recommended",
      "plugin:react/jsx-runtime",
      "plugin:prettier/recommended"
    ],
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module",
      "project": "./tsconfig.json",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "globals": {
      "ga": "readonly"
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    },
    "rules": {
      "prettier/prettier": "error",
      "no-unused-vars": "off",
      "react/prop-types": "off",
      "react/no-unknown-property": [
        "error",
        {
          "ignore": [
            "css"
          ]
        }
      ],
      "@typescript-eslint/explicit-module-boundary-types": "off",
      "@typescript-eslint/no-explicit-any": "off",
      "@typescript-eslint/no-var-requires": "off",
      "@typescript-eslint/no-use-before-define": "off",
      "@typescript-eslint/no-unused-vars": [
        "error",
        {
          "args": "after-used",
          "argsIgnorePattern": "^_"
        }
      ]
    }
  },
  "eslintIgnore": [
    ".cache/",
    "public/",
    "*.d.ts",
    "*.json"
  ],
  "release-it": {
    "verbose": 2,
    "hooks": {
      "before:init": "npm run build",
      "after:git:release": "npm run deploy"
    },
    "git": {
      "commitMessage": "chore: release ${version}",
      "requireBranch": "develop",
      "tagName": "${version}"
    },
    "npm": {
      "publish": false
    },
    "github": {
      "release": true,
      "tokenRef": "PERSONAL_WEBSITE_GITHUB_TOKEN"
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": "angular"
      }
    }
  }
}
