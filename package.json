{
  "name": "timmywil.com",
  "private": true,
  "description": "Personal blog and website for Timmy Willison",
  "version": "1.4.2",
  "author": "Timmy Willison <4timmywil@gmail.com>",
  "scripts": {
    "build": "gatsby clean && gatsby build",
    "commit": "git-cz",
    "eslint": "eslint .",
    "deploy": "gatsby build && gh-pages -d public -b master",
    "format": "prettier --write **/*.{ts,tsx,js}",
    "lint": "concurrently --raw \"npm run type-check\" \"npm run eslint\" \"npm run prettier\"",
    "lint:fix": "concurrently --raw \"npm run type-check\" \"lint-staged\"",
    "post": "node tasks/post.js",
    "prettier": "prettier --check \"**/*.tsx\" \"**/*.ts\" \"**/*.js\"",
    "release": "semantic-release",
    "schema": "graphql-codegen --config codegen.yml",
    "serve": "gatsby serve",
    "start": "gatsby clean && gatsby develop",
    "test": "npm run lint",
    "type-check": "tsc -p tsconfig.json --noEmit"
  },
  "dependencies": {
    "@emotion/core": "^10.0.28",
    "@emotion/styled": "^10.0.14",
    "gatsby": "^2.24.3",
    "gatsby-image": "^2.4.13",
    "gatsby-plugin-emotion": "^4.3.10",
    "gatsby-plugin-feed": "^2.5.11",
    "gatsby-plugin-google-analytics": "^2.3.11",
    "gatsby-plugin-manifest": "^2.4.18",
    "gatsby-plugin-offline": "^3.2.18",
    "gatsby-plugin-react-helmet": "^3.3.10",
    "gatsby-plugin-sharp": "^2.6.19",
    "gatsby-plugin-typescript": "^2.4.15",
    "gatsby-plugin-typography": "^2.5.10",
    "gatsby-remark-copy-linked-files": "^2.3.11",
    "gatsby-remark-images": "^3.3.19",
    "gatsby-remark-prismjs": "^3.5.10",
    "gatsby-remark-responsive-iframe": "^2.4.11",
    "gatsby-remark-smartypants": "^2.3.10",
    "gatsby-source-filesystem": "^2.3.19",
    "gatsby-transformer-remark": "^2.8.25",
    "gatsby-transformer-sharp": "^2.5.11",
    "prismjs": "^1.21.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.1.0",
    "react-typography": "^0.16.19",
    "serialize-javascript": "^4.0.0",
    "typeface-crimson-text": "^0.0.72",
    "typeface-rosario": "^1.1.4",
    "typescript": "^3.9.7",
    "typography": "^0.16.19",
    "typography-theme-twin-peaks": "^0.16.19"
  },
  "devDependencies": {
    "@commitlint/cli": "^9.1.1",
    "@commitlint/config-conventional": "^9.1.1",
    "@commitlint/prompt": "^9.1.1",
    "@graphql-codegen/cli": "^1.17.0",
    "@graphql-codegen/typescript": "^1.17.0",
    "@semantic-release/exec": "^5.0.0",
    "@semantic-release/git": "^9.0.0",
    "@types/react-dom": "^16.9.8",
    "@types/react-helmet": "^6.0.0",
    "@types/typography": "^0.16.2",
    "@typescript-eslint/eslint-plugin": "^3.6.1",
    "@typescript-eslint/parser": "^3.6.1",
    "commitizen": "^4.1.2",
    "concurrently": "^5.2.0",
    "eslint": "^7.4.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.3",
    "gh-pages": "^3.1.0",
    "graphql": "^14.7.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "lodash.template": "^4.5.0",
    "mkdirp": "^1.0.4",
    "prettier": "^2.0.5",
    "semantic-release": "^17.1.1"
  },
  "resolutions": {
    "lodash": ">=4.17.15",
    "serialize-javascript": ">=2.1.1",
    "vfile-message": ">=2.0.2"
  },
  "bugs": {
    "url": "https://github.com/timmywil/timmywil.com/issues"
  },
  "homepage": "https://github.com/timmywil/timmywil.com#readme",
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "main": "n/a",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/timmywil/timmywil.com.git"
  },
  "prettier": {
    "arrowParens": "always",
    "jsxBracketSameLine": true,
    "printWidth": 100,
    "trailingComma": "none",
    "semi": false,
    "singleQuote": true
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/@commitlint/prompt"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "env FORCE_COLOR=1 npm run lint:fix"
    }
  },
  "release": {
    "branches": [
      "develop"
    ],
    "ci": false,
    "npmPublish": false,
    "pre-commit": "npm run lint:fix",
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      "@semantic-release/git",
      [
        "@semantic-release/exec",
        {
          "publishCmd": "npm run deploy"
        }
      ],
      "@semantic-release/github"
    ],
    "tagFormat": "${version}"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "prettier --write",
      "eslint --fix ."
    ]
  }
}
